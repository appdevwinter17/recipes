<div class="page-header">
  <h3>Photos Of Recipe #<%= @photos_of_recipe.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Recipe</dt>
      <dd>
        <% if @photos_of_recipe.recipe.present? %>
          <a href="/recipes/<%= @photos_of_recipe.recipe_id %>">
            <%= @photos_of_recipe.recipe.post_id %>
          </a>
        <% end %>
      </dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/photos_of_recipes" class="btn btn-primary">
        Back
      </a>
      <a href="/photos_of_recipes/<%= @photos_of_recipe.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_photos_of_recipe/<%= @photos_of_recipe.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
